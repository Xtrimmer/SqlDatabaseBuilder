<Project Sdk="Microsoft.NET.Sdk">

  <PropertyGroup>
    <OutputType>Library</OutputType>
    <TargetFramework>netstandard2.0</TargetFramework>
    <PackageId>SqlDatabaseBuilder</PackageId>
    <PackageVersion>0.0.1</PackageVersion>
    <Authors>Xtrimmer</Authors>
    <Owners>Xtrimmer</Owners>
    <Title>SQL Server Database Builder</Title>
    <Description>Create database resources in a Sql Server database.</Description>
    <ProjectUrl>https://github.com/Xtrimmer/SqlDatabaseBuilder</ProjectUrl>
    <LicenseUrl>https://raw.githubusercontent.com/Xtrimmer/SqlDatabaseBuilder/master/LICENSE</LicenseUrl>
    <PackageRequireLicenseAcceptance>false</PackageRequireLicenseAcceptance>
    <GeneratePackageOnBuild>false</GeneratePackageOnBuild>
  </PropertyGroup>

  <ItemGroup>
    <PackageReference Include="System.Data.SqlClient" Version="4.6.0" />
  </ItemGroup>

</Project>
